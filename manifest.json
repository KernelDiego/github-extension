{
    "manifest_version": 3,
    "name": "GitHub Extension",
    "version": "1.0",
    "author": "KernelDiego",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "declarativeNetRequest"
    ],
    "icons": {
      "128": "img/icon-github-r-white.png"
    },
    "action": {
      "default_icon": "img/icon-github-r-white.png",
      "default_title": "GitHub Preview Code"
    }
}
